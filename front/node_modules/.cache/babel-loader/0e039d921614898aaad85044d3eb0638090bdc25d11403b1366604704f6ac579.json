{"ast":null,"code":"import { getAssignmentList, deleteAssignment, uploadAssignment } from \"./api.js\";\nimport add from \"./add.vue\";\nimport edit from \"./edit.vue\";\nimport uploadFileInput from \"@/views/template/upload-file-input\";\nexport default {\n  name: \"single-window\",\n  components: {\n    add,\n    edit,\n    uploadFileInput\n  },\n  data() {\n    return {\n      uploadModal: false,\n      uploadFile: \"\",\n      uploadLoading: false,\n      uploadObj: {},\n      tableHeight: 0,\n      selected: [\"选择\", \"序号\", \"课程名称\", \"作业名称\", \"作业附件\", \"完成附件\", \"创建时间\", \"创建者\", \"操作\"],\n      modal1: false,\n      openSearch: true,\n      // 显示搜索\n      openTip: true,\n      // 显示提示\n      formData: {},\n      currView: \"index\",\n      loading: true,\n      // 表单加载状态\n      searchForm: {\n        // 搜索框初始化对象\n        pageNumber: 1,\n        // 当前页数\n        pageSize: 15,\n        // 页面大小\n        sort: \"createTime\",\n        // 默认排序字段\n        order: \"desc\" // 默认排序方式\n      },\n      selectList: [],\n      // 多选数据\n      selectCount: 0,\n      // 多选计数\n      selectRow: 0,\n      columns: [\n      // 表头\n      {\n        type: \"selection\",\n        width: 60,\n        title: \"选择\",\n        align: \"center\",\n        fixed: \"left\"\n      }, {\n        title: \"序号\",\n        width: 85,\n        align: \"center\",\n        fixed: \"left\",\n        sortType: true,\n        render: (h, params) => {\n          return h(\"span\", params.index + (this.searchForm.pageNumber - 1) * this.searchForm.pageSize + 1);\n        }\n      }, {\n        title: \"课程ID\",\n        key: \"curriculumId\",\n        minWidth: 180,\n        tooltip: true,\n        sortable: false\n      }, {\n        title: \"课程名称\",\n        key: \"curriculumName\",\n        minWidth: 180,\n        tooltip: true,\n        sortable: false\n      }, {\n        title: \"作业名称\",\n        key: \"title\",\n        minWidth: 180,\n        tooltip: true,\n        sortable: false\n      }, {\n        title: \"作业附件\",\n        key: \"file1\",\n        minWidth: 180,\n        tooltip: true,\n        sortable: false,\n        render: (h, params) => {\n          if (params.row.file1 == undefined || params.row.file1 == \"\") {\n            return h(\"div\", [h(\"span\", {\n              style: {\n                color: \"#ff9900\"\n              }\n            }, \"未上传\")]);\n          } else {\n            return h(\"img\", {\n              attrs: {\n                src: require(\"@/assets/file.png\")\n              },\n              style: {\n                cursor: \"zoom-in\",\n                width: \"60px\",\n                margin: \"10px 0\",\n                \"object-fit\": \"contain\"\n              },\n              on: {\n                click: () => {\n                  this.showFile(params.row.file1);\n                }\n              }\n            });\n          }\n        }\n      }, {\n        title: \"完成附件\",\n        key: \"file2\",\n        minWidth: 180,\n        tooltip: true,\n        sortable: false,\n        render: (h, params) => {\n          if (params.row.file2 == undefined || params.row.file2 == \"\") {\n            return h(\"div\", [h(\"span\", {\n              style: {\n                color: \"#ff9900\"\n              }\n            }, \"未上传\")]);\n          } else {\n            return h(\"img\", {\n              attrs: {\n                src: require(\"@/assets/file.png\")\n              },\n              style: {\n                cursor: \"zoom-in\",\n                width: \"60px\",\n                margin: \"10px 0\",\n                \"object-fit\": \"contain\"\n              },\n              on: {\n                click: () => {\n                  this.showFile(params.row.file2);\n                }\n              }\n            });\n          }\n        }\n      }, {\n        title: \"创建时间\",\n        key: \"createTime\",\n        sortable: true,\n        sortType: \"desc\",\n        minWidth: 180,\n        align: \"center\",\n        tooltip: true\n      }, {\n        title: \"创建者\",\n        key: \"createBy\",\n        sortable: true,\n        sortType: \"desc\",\n        minWidth: 100,\n        align: \"center\",\n        tooltip: true\n      }, {\n        title: \"修改时间\",\n        key: \"updateTime\",\n        minWidth: 180,\n        align: \"center\",\n        tooltip: true\n      }, {\n        title: \"修改者\",\n        key: \"updateBy\",\n        minWidth: 100,\n        align: \"center\",\n        tooltip: true\n      }, {\n        title: \"操作\",\n        key: \"action\",\n        align: \"center\",\n        width: 290,\n        fixed: \"right\",\n        render: (h, params) => {\n          var that = this;\n          return h(\"div\", [h(\"Button\", {\n            props: {\n              type: \"success\",\n              size: \"small\",\n              icon: \"ios-create-outline\",\n              ghost: true,\n              disabled: !(that.$route.meta.permTypes && that.$route.meta.permTypes.includes(\"enable\"))\n            },\n            style: {\n              marginRight: \"5px\"\n            },\n            on: {\n              click: () => {\n                this.uploadObj = params.row;\n                this.uploadFile = \"\";\n                this.uploadModal = true;\n              }\n            }\n          }, \"作业上传\"), h(\"Button\", {\n            props: {\n              type: \"primary\",\n              size: \"small\",\n              icon: \"ios-create-outline\",\n              ghost: true\n            },\n            style: {\n              marginRight: \"5px\"\n            },\n            on: {\n              click: () => {\n                this.edit(params.row);\n              }\n            }\n          }, \"详情\"), h(\"Button\", {\n            props: {\n              type: \"error\",\n              size: \"small\",\n              icon: \"md-trash\",\n              ghost: true,\n              disabled: !(that.$route.meta.permTypes && that.$route.meta.permTypes.includes(\"delete\"))\n            },\n            on: {\n              click: () => {\n                this.remove(params.row);\n              }\n            }\n          }, \"删除\")]);\n        }\n      }],\n      data: [],\n      // 表单数据\n      pageNumber: 1,\n      // 当前页数\n      pageSize: 10,\n      // 页面大小\n      total: 0,\n      // 表单数据总数\n      showFilterPanelFlag: false\n    };\n  },\n  methods: {\n    init() {\n      this.getDataList();\n    },\n    uploadAssignmentFx() {\n      var that = this;\n      that.uploadLoading = true;\n      uploadAssignment({\n        id: that.uploadObj.id,\n        file: that.uploadFile\n      }).then(res => {\n        that.uploadLoading = false;\n        that.uploadModal = false;\n        if (res.success) {\n          this.$Message.success(\"上传成功\");\n          that.getDataList();\n        }\n      });\n    },\n    submited() {\n      this.currView = \"index\";\n      this.getDataList();\n    },\n    changePage(v) {\n      this.searchForm.pageNumber = v;\n      this.getDataList();\n      this.clearSelectAll();\n    },\n    changePageSize(v) {\n      this.searchForm.pageSize = v;\n      this.getDataList();\n    },\n    rowClick(row, index) {\n      this.selectRow = row;\n    },\n    rowClassNmae(row, index) {\n      if (row.id == this.selectRow.id) {\n        return \"rowClassNmaeColor\";\n      }\n      return \"\";\n    },\n    excelData() {\n      this.$refs.table.exportCsv({\n        filename: \"导出结果\"\n      });\n    },\n    handleSearch() {\n      this.searchForm.pageNumber = 1;\n      this.searchForm.pageSize = 15;\n      this.getDataList();\n    },\n    handleReset() {\n      this.$refs.searchForm.resetFields();\n      this.searchForm.pageNumber = 1;\n      this.searchForm.pageSize = 15;\n      // 重新加载数据\n      this.getDataList();\n    },\n    changeSort(e) {\n      this.searchForm.sort = e.key;\n      this.searchForm.order = e.order;\n      if (e.order === \"normal\") {\n        this.searchForm.order = \"\";\n      }\n      this.getDataList();\n    },\n    clearSelectAll() {\n      this.$refs.table.selectAll(false);\n    },\n    changeSelect(e) {\n      this.selectList = e;\n      this.selectCount = e.length;\n    },\n    getDataList() {\n      this.loading = true;\n      getAssignmentList(this.searchForm).then(res => {\n        this.loading = false;\n        if (res.success) {\n          this.data = res.result.records;\n          this.total = res.result.total;\n        }\n      });\n    },\n    add() {\n      this.currView = \"add\";\n    },\n    edit(v) {\n      // 转换null为\"\"\n      for (let attr in v) {\n        if (v[attr] == null) {\n          v[attr] = \"\";\n        }\n      }\n      let str = JSON.stringify(v);\n      let data = JSON.parse(str);\n      this.formData = data;\n      this.currView = \"edit\";\n    },\n    remove(v) {\n      this.$Modal.confirm({\n        title: \"确认删除\",\n        // 记得确认修改此处\n        content: \"您确认要删除 ?\",\n        loading: true,\n        onOk: () => {\n          // 删除\n          deleteAssignment({\n            ids: v.id\n          }).then(res => {\n            this.$Modal.remove();\n            if (res.success) {\n              this.$Message.success(\"操作成功\");\n              this.getDataList();\n            }\n          });\n        }\n      });\n    },\n    delAll() {\n      if (this.selectCount <= 0) {\n        this.$Message.warning(\"您还未选择要删除的数据\");\n        return;\n      }\n      this.$Modal.confirm({\n        title: \"确认删除\",\n        content: \"您确认要删除所选的 \" + this.selectCount + \" 条数据?\",\n        loading: true,\n        onOk: () => {\n          let ids = \"\";\n          this.selectList.forEach(function (e) {\n            ids += e.id + \",\";\n          });\n          ids = ids.substring(0, ids.length - 1);\n          // 批量删除\n          deleteAssignment({\n            ids: ids\n          }).then(res => {\n            this.$Modal.remove();\n            if (res.success) {\n              this.$Message.success(\"操作成功\");\n              this.clearSelectAll();\n              this.getDataList();\n            }\n          });\n        }\n      });\n    },\n    showFile(e) {\n      window.open(e + \"?preview=true\");\n    }\n  },\n  mounted() {\n    this.init();\n    this.tableHeight = Number(window.innerHeight - 273);\n    this.mycolumns = this.columns;\n    let showcolumns = [];\n    for (var i = 0; i < this.selected.length; i++) {\n      var item = this.selected[i];\n      for (var j = 0; j < this.columns.length; j++) {\n        if (this.columns[j].title == item) {\n          showcolumns.push(this.columns[j]);\n        }\n      }\n    }\n    this.columns = showcolumns;\n  },\n  watch: {\n    selected: function (newcolumns) {\n      let showcolumns = [];\n      for (var i = 0; i < this.mycolumns.length; i++) {\n        var item = this.mycolumns[i];\n        if (item.title == undefined) showcolumns.push(item);else if (newcolumns.includes(item.title)) showcolumns.push(item);\n      }\n      this.columns = showcolumns;\n    }\n  }\n};","map":{"version":3,"names":["getAssignmentList","deleteAssignment","uploadAssignment","add","edit","uploadFileInput","name","components","data","uploadModal","uploadFile","uploadLoading","uploadObj","tableHeight","selected","modal1","openSearch","openTip","formData","currView","loading","searchForm","pageNumber","pageSize","sort","order","selectList","selectCount","selectRow","columns","type","width","title","align","fixed","sortType","render","h","params","index","key","minWidth","tooltip","sortable","row","file1","undefined","style","color","attrs","src","require","cursor","margin","on","click","showFile","file2","that","props","size","icon","ghost","disabled","$route","meta","permTypes","includes","marginRight","remove","total","showFilterPanelFlag","methods","init","getDataList","uploadAssignmentFx","id","file","then","res","success","$Message","submited","changePage","v","clearSelectAll","changePageSize","rowClick","rowClassNmae","excelData","$refs","table","