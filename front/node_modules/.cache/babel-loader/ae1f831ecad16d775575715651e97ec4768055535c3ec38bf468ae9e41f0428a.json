  uploadPicInput,\r\n    },\r\n    data() {\r\n        return {\r\n            submitLoading: false, // 表单提交状态\r\n            form: { // 添加或编辑表单对象初始化数据\r\n                curriculumId: \"\",\r\n                curriculumName: \"\",\r\n                title: \"\",\r\n                fileUrl: \"\",\r\n            },\r\n            // 表单验证规则\r\n            formValidate: {},\r\n            curriculumList: []\r\n        };\r\n    },\r\n    methods: {\r\n        init() {\r\n            this.getCurriculumListFx();\r\n        },\r\n        getCurriculumListFx() {\r\n            var that = this;\r\n            that.curriculumList = [];\r\n            getCurriculumList().then(res => {\r\n                if (res.success) {\r\n                    that.curriculumList = res.result;\r\n                }\r\n            })\r\n        },\r\n        handleReset() {\r\n            this.$refs.form.resetFields();\r\n        },\r\n        handleSubmit() {\r\n            this.$refs.form.validate(valid => {\r\n                if (valid) {\r\n                    addCourseResources(this.form).then(res => {\r\n                        this.submitLoading = false;\r\n                        if (res.success) {\r\n                            this.$Message.success(\"操作成功\");\r\n                            this.submited();\r\n                        }\r\n                    });\r\n                }\r\n            });\r\n        },\r\n        close() {\r\n            this.$emit(\"close\", true);\r\n        },\r\n        submited() {\r\n            this.$emit(\"submited\", true);\r\n        }\r\n    },\r\n    mounted() {\r\n        this.init();\r\n    }\r\n};\r\n</script>\r\n\r\n<style lang=\"less\">\r\n// 建议引入通用样式 具体路径自行修改 可删除下面样式代码\r\n// @import \"../../../styles/single-common.less\";\r\n.edit-head {\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: space-between;\r\n    position: relative;\r\n\r\n    .back-title {\r\n        color: #515a6e;\r\n        display: flex;\r\n      