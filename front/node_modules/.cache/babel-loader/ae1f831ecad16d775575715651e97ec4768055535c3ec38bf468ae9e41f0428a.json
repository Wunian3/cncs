{"ast":null,"code":"import { addCourseResources, getCurriculumList } from \"./api.js\";\nimport uploadPicInput from \"@/views/template/upload-file-input\";\nexport default {\n  name: \"add\",\n  components: {\n    uploadPicInput\n  },\n  data() {\n    return {\n      submitLoading: false,\n      // 表单提交状态\n      form: {\n        // 添加或编辑表单对象初始化数据\n        curriculumId: \"\",\n        curriculumName: \"\",\n        title: \"\",\n        fileUrl: \"\"\n      },\n      // 表单验证规则\n      formValidate: {},\n      curriculumList: []\n    };\n  },\n  methods: {\n    init() {\n      this.getCurriculumListFx();\n    },\n    getCurriculumListFx() {\n      var that = this;\n      that.curriculumList = [];\n      getCurriculumList().then(res => {\n        if (res.success) {\n          that.curriculumList = res.result;\n        }\n      });\n    },\n    handleReset() {\n      this.$refs.form.resetFields();\n    },\n    handleSubmit() {\n      this.$refs.form.validate(valid => {\n        if (valid) {\n          addCourseResources(this.form).then(res => {\n            this.submitLoading = false;\n            if (res.success) {\n              this.$Message.success(\"操作成功\");\n              this.submited();\n            }\n          });\n        }\n      });\n    },\n    close() {\n      this.$emit(\"close\", true);\n    },\n    submited() {\n      this.$emit(\"submited\", true);\n    }\n  },\n  mounted() {\n    this.init();\n  }\n};","map":{"version":3,"names":["addCourseResources","getCurriculumList","uploadPicInput","name","components","data","submitLoading","form","curriculumId","curriculumName","title","fileUrl","formValidate","curriculumList","methods","init","getCurriculumListFx","that","then","res","success","result","handleReset","$refs","resetFields","handleSubmit","validate","valid","$Message","submited","close","$emit","mounted"],"sources":["src/views/study/courseResources/add.vue"],"sourcesContent":["<template>\r\n<div>\r\n    <Card>\r\n        <div slot=\"title\">\r\n  