{"ast":null,"code":"import sidebarMenu from \"./components/sidebarMenu.vue\";\nimport sidebarMenuShrink from \"./components/sidebarMenuShrink.vue\";\nimport util from \"@/libs/util\";\nexport default {\n  name: \"shrinkableMenu\",\n  components: {\n    sidebarMenu,\n    sidebarMenuShrink\n  },\n  props: {\n    shrink: {\n      type: Boolean,\n      default: false\n    },\n    menuList: {\n      type: Array,\n      required: true\n    },\n    // theme: {\n    //   type: String,\n    //   default: \"dark\",\n    //   validator(val) {\n    //     return util.oneOf(val, [\"dark\", \"light\"]);\n    //   }\n    // },\n    beforePush: {\n      type: Function\n    },\n    openNames: {\n      type: Array\n    }\n  },\n  computed: {\n    bgColor() {\n      return \"#e6e6e6\";\n    },\n    shrinkIconColor() {\n      return this.theme == \"dark\" ? \"#fff\" : \"#e6e6e6\";\n    }\n  },\n  methods: {\n    handleChange(name) {\n      let willpush = true;\n      if (this.beforePush !== undefined) {\n        if (!this.beforePush(name)) {\n          willpush = false;\n        }\n      }\n      if (willpush) {\n        this.$router.push({\n          name: name\n        });\n      }\n      this.$emit(\"on-change\", name);\n    }\n  }\n};","map":{"version":3,"names":["sidebarMenu","sidebarMenuShrink","util","name","components","props","shrink","type","Boolean","default","menuList","Array","required","beforePush","Function","openNames","computed","bgColor","shrinkIconColor","theme","methods","handleChange","willpush","undefined","$router","push","$emit"],"sources":["src/views/main-components/shrinkable-menu/shrinkable-menu.vue"],"sourcesContent":["<style lang=\"less\">\r\n@import \"./styles/menu.less\";\r\n</style>\r\n\r\n<template>\r\n  <div :style=\"{background: bgColor}\" class=\"ivu-shrinkable-menu\">\r\n    <slot name=\"top\"></slot>\r\n    <sidebar-menu\r\n      v-show=\"!shrink\"\r\n      :menu-theme=\"theme\"\r\n      :menu-list=\"menuList\"\r\n      :open-names=\"openNames\"\r\